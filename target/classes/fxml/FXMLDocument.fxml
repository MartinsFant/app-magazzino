<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" stylesheets="" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="marti.magazzino.gui.main.FXMLDocumentController">
   <children>
      <HBox prefHeight="329.0" prefWidth="800.0">
         <children>
            <VBox alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" prefHeight="498.0" prefWidth="780.0" spacing="10.0">
               <children>
                  <Label text="Choose an option:" textAlignment="CENTER">
                     <font>
                        <Font name="System Bold" size="22.0" />
                     </font>
                  </Label>
                  <RadioButton fx:id="warehouseButton" mnemonicParsing="false" onAction="#radioButtonChanged" prefHeight="21.0" prefWidth="176.0" text="Warehouse" />
                  <RadioButton fx:id="barButton" mnemonicParsing="false" onAction="#radioButtonChanged" prefHeight="21.0" prefWidth="178.0" text="Bar" />
                  <RadioButton fx:id="generalProductListButton" layoutX="349.0" layoutY="188.0" mnemonicParsing="false" onAction="#radioButtonChanged" prefHeight="21.0" prefWidth="177.0" text="General Product List" />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <HBox layoutX="7.0" layoutY="329.0" prefHeight="250.0" prefWidth="771.0" spacing="20.0">
         <children>

            <AnchorPane prefHeight="250.0" prefWidth="860.0">
               <children>
                  <Button alignment="BOTTOM_CENTER" layoutX="335.0" layoutY="225.0" mnemonicParsing="false" onAction="#changeScreenButtonPushed" prefHeight="30.0" prefWidth="102.0" text="Go to:" textAlignment="CENTER" />
                  <Label fx:id="radioButtonLabel" layoutX="453.0" layoutY="226.0" prefHeight="21.0" prefWidth="106.0" text="Label">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
